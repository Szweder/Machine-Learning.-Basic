from car import Car
from engine import Engine

car = Car(weight=1200, fuel=50, fuel_consumption=5)
engine = Engine(volume=1.6, pistons=4)
car.set_engine(engine)
car.start()
car.move(5)  # –†–∞—Å—Ö–æ–¥—É–µ—Ç 25 —Ç–æ–ø–ª–∏–≤–∞
